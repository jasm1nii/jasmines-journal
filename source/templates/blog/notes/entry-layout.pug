doctype html
head
  meta(charset='UTF-8')
  meta(name='viewport' content='width=device-width, initial-scale=1.0')
  <!--#include virtual="/assets/includes/head.shtml"-->
  title
    block post title
    |  | jasmine&apos;s notes
  link(rel='stylesheet' href='/assets/stylesheets/style-10.css' type='text/css' media='all')
  block post styles
  block bridgy slug
  meta(name='author' content='jasm1nii')
  meta(name='keywords' content='microblog')
  block meta description
body
  #layout
    header
      hgroup
        h1 jasmine&apos;s notes
      nav(aria-label='primary')
        <!--#include virtual="/assets/includes/headernav.shtml"-->
    main.notes-entry
      article
        h2
          block publish date
        .h-entry
          p.e-content
            block post content
          a.u-bridgy-fed(href='https://fed.brid.gy/' aria-hidden='true' tabindex='-1')
      section#webmentions
        h3 webmentions
        <!--#include virtual="/assets/includes/webmentions-module.shtml"-->
      section#comments
        h3 comments
        #comment-section
  footer
    <!--#include virtual="/assets/includes/footer.shtml"-->
  <!--#include virtual="/assets/includes/webmentions-script.shtml"-->
  <!--#include virtual="/assets/includes/comments-script.shtml"-->
  script(src='index.js')
  script(src='/assets/scripts/theme-switcher-v2.js')